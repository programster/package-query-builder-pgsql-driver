{
    "name": "programster/query-builder-pgsql-driver",
    "type": "library",
    "description": "A PostgreSQL driver for the programster/query-builder-parser package.",
    "keywords": ["postgresql","query", "builder"],
    "homepage": "https://github.com/programster/package-query-builder-pgsql-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "email": "sdpagent@gmail.com",
            "homepage": "https://blog.programster.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "programster/pgsql-lib": "1.0.*",
         "ext-pgsql": "*",
        "programster/query-builder-parser": "1.0.*"
    },
    "require-dev": {
        "programster/core-libs": "^2.1",
        "irap/autoloader": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Programster\\QueryBuilderPgsqlDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programster\\QueryBuilderPgsqlDriver\\Testing\\": "testing/",
            "Programster\\QueryBuilderPgsqlDriver\\Testing\\Tests\\": "testing/tests/"
        }
    }
}
